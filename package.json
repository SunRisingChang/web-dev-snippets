{
  "name": "web-dev-snippets",
  "displayName": "Web Dev Snippets",
  "description": "Web 开发常用的代码片段。",
  "version": "0.1.1",
  "publisher": "SunRising",
  "icon": "asset/logo.png",
  "scripts": {
    "out": "node src/out.js",
    "build": "vsce package",
    "update": "node src/update-snippets.js"
  },
  "repository": "https://github.com/SunRisingChang/web-dev-snippets",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "html",
        "path": "./snippets/html.json"
      },
      {
        "language": "vue-html",
        "path": "./snippets/html.json"
      },
      {
        "language": "vue",
        "path": "./snippets/html.json"
      }
    ]
  },
  "devDependencies": {
    "chalk": "^4.0.0",
    "cheerio": "^1.0.0-rc.3",
    "request": "^2.88.2",
    "vsce": "^1.75.0"
  }
}